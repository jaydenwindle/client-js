{
  "name": "polygon.io",
  "version": "1.0.0",
  "description": "Node JS client for polygon.io api",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "mocha --require ts-node/register '**/*.test.ts'",
    "test:watch": "mocha --require ts-node/register --watch '**/*.test.ts'",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bassochette/polygon.io.git"
  },
  "keywords": [
    "polygon.io"
  ],
  "author": "Julien Prugne <julien@webeleon.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bassochette/polygon.io/issues"
  },
  "homepage": "https://github.com/bassochette/polygon.io#readme",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/sinon": "^7.5.0",
    "@types/websocket": "^1.0.0",
    "@types/ws": "^6.0.3",
    "chai": "^4.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.1",
    "prettier": "1.18.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "querystring": "^0.2.0",
    "websocket": "^1.0.31",
    "ws": "^7.1.2"
  }
}
